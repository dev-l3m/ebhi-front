{
  "name": "ebhi-front",
  "version": "1.0.0",
  "type": "module",
  "description": "EBHI - Modern offshore outsourcing platform",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --ignore-path .gitignore",
    "format": "prettier --write \"src/**/*.{js,vue,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,vue,json,css,scss,md}\"",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "quality": "yarn lint:check && yarn format:check && yarn build",
    "prepare": "husky install"
  },
  "dependencies": {
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vuetify": "^3.5.10",
    "@mdi/font": "^7.4.47",
    "pinia": "^2.1.7"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "vite": "^5.1.6",
    "vite-plugin-vuetify": "^2.0.2",
    "@vue/eslint-config-prettier": "^9.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.20.1",
    "prettier": "^3.2.5",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.0"
  }
}
